{
  "name": "react-elemental",
  "version": "0.0.1",
  "description": "Flat UI library for React",
  "main": "dist/react-elemental.js",
  "scripts": {
    "build": "webpack --config config/webpack-lib.js",
    "build-watch": "npm run build -- --watch",
    "start": "webpack-dev-server --content-base sample/ --config config/webpack-sample.js --watch",
    "lint": "eslint src test sample config",
    "test": "jest test",
    "cover": "jest --coverage test",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LINKIWI/react-elemental.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "concurrently": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.6.1"
  },
  "peerDependencies": {
    "react": "^15.6.1"
  }
}
